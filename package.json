      },
      {
        "name": "secondSet",
        "defaultFile": "sounds/secondSet.ogg",
        "assignable": false
      },
      {
        "name": "lockin",
        "defaultFile": "sounds/lockin.ogg",
        "assignable": false
      },
      {
        "name": "teamOneBuzz"
      },
      {
        "name": "teamTwoBuzz"
      }
