    "graphics": [
      {
        "file": "lower_third.html",
        "width": 1920,
        "height": 250,
        "singleInstance": false
      },
      {
        "file": "question.html",
        "width": 1920,
        "height": 250,
        "singleInstance": false
      },
      {
        "file": "scoreboard.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      },
      {
        "file": "feedback.html",
