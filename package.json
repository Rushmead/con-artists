      },
      {
        "name": "lower-third",
        "title": "Lower Third",
        "width": 4,
        "file": "lower_third.html",
        "headerColor": "#9f9bbd",
        "workspace": "Settings"
      },
      {
        "name": "questions",
        "title": "Questions System",
        "width": 4,
        "file": "questions/question.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "messaging",
        "title": "Messaging",
        "width": 4,
        "file": "messaging/messaging.html",
        "headerColor": "#9f9bbd"
      },
      {
        "name": "sounds",
        "title": "Sounds",
        "width": 2,
        "file": "sounds/sounds.html",
